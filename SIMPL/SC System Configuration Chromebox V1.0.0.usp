FUNCTION Initialize()
{
	RegisterDelegate(configUpdater,UpdateChromeboxInformation,UpdateChromeboxInformationCallback);
	RegisterDelegate(configUpdater,UpdateChromeboxRoutingInformation, UpdateChromeboxRoutingInformationCallback);
}

callback FUNCTION UpdateChromeboxRoutingInformationCallback(integer chromeboxInput, integer hqPresentOutput, integer camInput)
{
	integer tempCfmInput;
	integer tempHqPresentOutput;
	integer tempCamInput;

	tempCfmInput = chromeboxInput;
	tempHqPresentOutput = hqPresentOutput;
	tempCamInput = camInput;

	ChromeboxInputAnalog = tempCfmInput;
	ChromeboxInputText = itoa(tempCfmInput);

	HqPresentOutputAnalog = tempHqPresentOutput;
	HqPresentOutputText = itoa(tempHqPresentOutput);

	CamInputAnalog = tempCamInput;
	CamInputText = itoa(tempCamInput);
}
 
