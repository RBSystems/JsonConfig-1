CHANGE CameraInput
{
	integer in;

	in = atoi(CameraInput);

	if(in > 0)
		configUpdater.SetCameraInput(in);
}

CHANGE ChromeboxInput
{
	integer in;

	in = atoi(ChromeboxInput);
	
	if(in > 0)
		configUpdater.SetChromeboxInput(in);
}

CHANGE HqPresentOutput
{
	integer out;

	out = atoi(HqPresentOutput);

	if(out > 0)
		configUpdater.SetHqPresentationOutput(out);
}

CHANGE ChromeboxEnable
{
    configUpdater.EnableChromebox(ChromeboxEnable);
}

CHANGE HqPresentEnable
{
	configUpdater.EnableHqPresentation(HqPresentEnable);
}

